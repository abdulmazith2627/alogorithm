void main() {
  int X = 1000;
  
  Map<String, int> country = {
    'japan': 1000,
    'brazil': 300,
    'australia': 500,
  };
  
  List<String> getcountryresult = [];
  

  List<MapEntry<String, int>> getdata = country.entries.toList()
    ..sort((a, b) => a.value.compareTo(b.value));
    
  for (var i in getdata) {
    if (X >= i.value) {
      getcountryresult.add(i.key); 
      X -= i.value;  
    }
  }


  print(getcountryresult);// output : [brazil, australia]
}

