void main() {
  List<int> trail = [3, 2, 1];
  List<int> boots = [4, 5, 3];

  int current=-1;
  
  for(int i = 0; i < trail.length; i++){
    int getbootnew=boots.indexWhere((boot)=>boot>=trail[i]); // check boots >= trail value
      print("new : ${getbootnew}"); // trail value is not Greater than boots so i get value 0
    if(getbootnew==-1){ 
      print(-1);// when getnewdata is value   -1 i print -1
    }else{
      if(current!=getbootnew){ // check current value is != getbootnew 
        current =getbootnew; // getbootnew data is store on current     
      }
    }
  
}
      print("current : ${current}");
}
